{%- liquid 
    assign sid = section.id
    assign se_stts = section.settings
    assign se_height = se_stts.se_height
    assign video_url = se_stts.video_url | default:"https://www.youtube.com/watch?v=_9VUPq3SxOc" 
    assign source1 = se_stts.source1
    assign source2 = se_stts.source2
    assign source3 = se_stts.source3
    assign source4 = se_stts.source4
    assign url1 = se_stts.video_url1 | remove: "https://youtu.be/" | prepend: "embed/" | prepend: "//www.youtube.com/" | append: "?autoplay=1&mute=1&start=5&showinfo=0&controls=0&modestbranding=1&loop=1" 
    assign url2 = se_stts.video_url2 | remove: "https://youtu.be/" | prepend: "embed/" | prepend: "//www.youtube.com/" | append: "?autoplay=1&mute=1&start=5&showinfo=0&controls=0&modestbranding=1&loop=1" 
    assign url3 = se_stts.video_url3 | remove: "https://youtu.be/" | prepend: "embed/" | prepend: "//www.youtube.com/" | append: "?autoplay=1&mute=1&start=5&showinfo=0&controls=0&modestbranding=1&loop=1" 
    assign url4 = se_stts.video_url4 | remove: "https://youtu.be/" | prepend: "embed/" | prepend: "//www.youtube.com/" | append: "?autoplay=1&mute=1&start=5&showinfo=0&controls=0&modestbranding=1&loop=1" 
    assign urltest = "https://www.youtube.com/embed?v=-2icZZ9tbwc?autoplay=1"
    assign image1 = se_stts.image1
    assign image2 = se_stts.image2
    assign image3 = se_stts.image3
    assign image4 = se_stts.image4 -%}

  <div class="sk-collection-top" style="margin-bottom: 100px;">
    <div class="row equal_nt auto al_center"></div>
      <div class="mobile-remove-left-padding col-md-6 txt_col_img" style="max-width: 100%; padding-right: 0">
        <div class="container best-seller-img-container">
          <div class="sk-best-seller-img-slider products nt_products_holder dot_owl_{{se_stts.dot_owl}} dot_color_{{se_stts.dot_color}} btn_vi_{{se_stts.btn_vi}} row row_pr_{{inc_pr}} cdt_des_{{se_stts.cdt_des}} round_cd_{{se_stts.round_cd}} js_carousel nt_slider {{se_stts.image_size}} {{se_stts.image_ratio}} position_{{se_stts.image_position}} space_{{se_stts.space}} prev_next_{{se_stts.btn_color}} btn_owl_{{se_stts.btn_owl}} dot_owl_{{se_stts.dot_owl}} dot_color_{{se_stts.dot_color}}{% if sett_equal %} equal_nt{% endif %}  is-draggable" data-flickityt4s-js='{"imagesLoaded": 0,"adaptiveHeight": 0, "contain": 1, "groupCells": "100%", "dragThreshold" : 5, "cellAlign": "left","wrapAround": true,"prevNextButtons": {{se_stts.button}},"percentPosition": 1,"pageDots": {{se_stts.dot}}, "autoPlay" : {{ se_stts.au_time | times: 1000 }}, "pauseAutoPlayOnHover" : {{se_stts.au_hover}}, "rightToLeft": {{settings.use_rtl}} }'>
            <button class="flickityt4s-button flickityt4s-prev-next-button previous" type="button" aria-label="Previous" style="z-index: 99999; visibility: visible;">
                <svg class="flickityt4s-button-icon" viewBox="0 0 100 100">
                    <path d="M 10,50 L 60,100 L 70,90 L 30,50  L 70,10 L 60,0 Z" class="arrow" transform="translate(100, 100) rotate(180)"></path>
                </svg>
            </button>
            <button class="flickityt4s-button flickityt4s-prev-next-button next" type="button" aria-label="Next" style="z-index: 99999;visibility: visible">
                <svg class="flickityt4s-button-icon" viewBox="0 0 100 100">
                    <path d="M 10,50 L 60,100 L 70,90 L 30,50  L 70,10 L 60,0 Z" class="arrow"></path>
                </svg>
            </button>
            {% if url1 or url2 or url3 or url4 %}

            {%- if url1  -%}
            <div class="sendero-session-slider-vid-container">
                <iframe width="100%" height="100%" src="{{url1}}" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>     
                </iframe>  
                  <h5 class="sendero-session-sub-title">WACO, TEXAS</h5>
                  <h3 class="sendero-session-title">{{se_stts.file_name1}}</h3>
                  <div class="sendero-session-overlay"></div>
            </div>
              {%- endif -%}

              {%- if url2  -%}
              <div class="sendero-session-slider-vid-container">
                <iframe width="100%" height="100%" src="{{url2}}" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>     
                </iframe>   
              <h5 class="sendero-session-sub-title">WACO, TEXAS</h5>
              <h3 class="sendero-session-title">{{se_stts.file_name2}}</h3>
              <div class="sendero-session-overlay"></div>
              </div>
              {%- endif -%}

              {%- if url3  -%}
              <div class="sendero-session-slider-vid-container">
                <iframe width="100%" height="100%" src="{{url3}}" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>     
                </iframe>   
              <h5 class="sendero-session-sub-title">WACO, TEXAS</h5>
              <h3 class="sendero-session-title">{{se_stts.file_name3}}</h3>
              <div class="sendero-session-overlay"></div>
              </div>
              {%- endif -%}

              {%- if url4  -%}
              <div class="sendero-session-slider-vid-container">
                <iframe width="100%" height="100%" src="{{url4}}" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>     
                </iframe>      
              <h5 class="sendero-session-sub-title">WACO, TEXAS</h5>
              <h3 class="sendero-session-title">{{se_stts.file_name4}}</h3>
              <div class="sendero-session-overlay"></div>
            </div>
              {%- endif -%}  

            {%- else -%}
              {%- for i in (1..8) -%}
                <div class="pr_animated mt__30 pr_grid_item product nt_pr desgin__1 col-6 col-lg-3 col-md-3 done">
                  <div class="product-inner pr"><div class="product-image pr oh"><a class="db" href="/admin/products">{%- capture current -%}{%- cycle 1, 2, 3, 4, 5, 6,7,8 -%}{%- endcapture -%}<div class="pr_lazy_img main-img nt_img_ratio nt_bg_lz lazyloadt4s" style="padding-top:100%" data-bg="{{  video_url }}"></div></a></div></div>
                </div>
              {%- endfor -%}
            {%- endif -%}
          </div>
        </div>
      </div>
    </div>



  {%- render 'section_css' -%}
  {%- style -%}
     .nt_se_{{sid}} .nt_img_txt>a:after{ background-color: {{se_stts.cl_overlay}};opacity: {{se_stts.cl_opacity | divided_by: 100.00}} }
   {%- if se_height == 'cus_h' -%}
        {%- if se_stts.use_custom_h == '2' -%}{% assign rto = ratio | default:3 %}{%- assign pd_top = 1 | divided_by: ratio | times: 100 -%}
          .nt_se_{{sid}}.se_height_cus_h .nt_bg_lz{padding-top:{{ se_stts.h_mb | replace:'px','' | strip }}px}
          @media (min-width: 768px) { .nt_se_{{sid}}.se_height_cus_h .nt_bg_lz{padding-top:{{ se_stts.h_tb | replace:'px','' | strip }}px} }
          @media (min-width: 1025px) { .nt_se_{{sid}}.se_height_cus_h .nt_bg_lz{padding-top:{{pd_top}}% } }
          {%- elsif se_stts.use_custom_h == '3' -%}{% assign rto = ratio | default:3 %}{%- assign pd_top = 1 | divided_by: ratio | times: 100 -%}
          .nt_se_{{sid}}.se_height_cus_h .nt_bg_lz{padding-top:{{ se_stts.h_mb | replace:'px','' | strip }}px}
          @media (min-width: 768px) { .nt_se_{{sid}}.se_height_cus_h .nt_bg_lz{padding-top:{{pd_top}}% } }
          @media (min-width: 1025px) { .nt_se_{{sid}}.se_height_cus_h .nt_bg_lz{padding-top:{{pd_top}}% } }
        {%- else -%}
          .nt_se_{{sid}}.se_height_cus_h .nt_bg_lz{padding-top:{{ se_stts.h_mb | replace:'px','' | strip }}px}
          @media (min-width: 768px) { .nt_se_{{sid}}.se_height_cus_h .nt_bg_lz{padding-top:{{ se_stts.h_tb | replace:'px','' | strip }}px} }
          @media (min-width: 1025px) { .nt_se_{{sid}}.se_height_cus_h .nt_bg_lz{padding-top:{{ se_stts.h_des | replace:'px','' | strip }}px} }
        {%- endif -%}
    {%- endif -%}
  
    {%- for block in section.blocks -%}{%- assign bid = block.id -%}{% assign bk_stts = block.settings -%}
        {%- case block.type -%}
          {%- when '1' -%}
          .nt_se_{{sid}} .nt_img_txt #b_{{bid}}{ {% if bk_stts.ls_mb_text != 0 %}letter-spacing:{{bk_stts.ls_mb_text}}px;{% endif %}{% if bk_stts.lh_mb_text != 0 %}line-height:{{bk_stts.lh_mb_text}}px;{% endif %}font-size: {{bk_stts.fs_mb_text}}px;font-weight: {{bk_stts.fw_text}};color: {{bk_stts.color}} }
          @media (min-width: 768px) {.nt_se_{{sid}} .nt_img_txt #b_{{bid}} { {% if bk_stts.ls_text != 0 %}letter-spacing:{{bk_stts.ls_text}}px;{% endif %}{% if bk_stts.lh_text != 0 %}line-height: {{bk_stts.lh_text}}px;{% endif %}font-size: {{bk_stts.fs_text}}px}}
            
          {%- when '2' -%}
          .nt_se_{{sid}} .nt_img_txt #b_{{bid}}{ {% if bk_stts.ls_mb_text != 0 %}letter-spacing:{{bk_stts.ls_mb_text}}px;{% endif %}{% if bk_stts.lh_mb_text != 0 %}line-height:{{bk_stts.lh_mb_text}}px;{% endif %}font-size: {{bk_stts.fs_mb_text}}px;font-weight: {{bk_stts.fw_text}};color: {{bk_stts.color}} }
          @media (min-width: 768px) {.nt_se_{{sid}} .nt_img_txt #b_{{bid}} { {% if bk_stts.ls_text != 0 %}letter-spacing:{{bk_stts.ls_text}}px;{% endif %}{% if bk_stts.lh_text != 0 %}line-height: {{bk_stts.lh_text}}px;{% endif %}font-size: {{bk_stts.fs_text}}px}}
  
          {%- when '4' -%}
          #b_{{bid}} {width: {{bk_stts.w_mb}}px;margin: {{bk_stts.sp_mb}}px 0 {{bk_stts.spb_mb}}px;height: {{bk_stts.h_mb}}px;background-color: {{bk_stts.color}}}
         @media (min-width: 768px) { #b_{{bid}} {width: {{bk_stts.w}}px;margin: {{bk_stts.sp}}px 0 {{bk_stts.spb}}px;height: {{bk_stts.h}}px} }
  
          {%- when '6' -%}
          #b_{{bid}} {height: {{bk_stts.sp_mb}}px}
          @media (min-width: 768px) {#b_{{bid}} { height:{{bk_stts.sp}}px} }
  
          {%- when '8'  %}{% assign image = bk_stts.image %}{% if image == blank %}{% continue %}{% endif %}#b_{{bid}} img{ max-width:{% if bk_stts.width_mb > 0 %}{{bk_stts.width_mb}}{% else %}{{image.width}}{% endif %}px}@media (min-width: 768px) {#b_{{bid}} img{ max-width:{% if bk_stts.width > 0 %}{{bk_stts.width}}{% else %}{{image.width}}{% endif %}px} }
  
          {%- when '3' -%}
           .nt_se_{{sid}} .imtt4_btn{
             padding-left:{{bk_stts.pd_mb}}px;padding-right:{{bk_stts.pd_mb}}px;border-radius:{{bk_stts.bdr_mb}}px;{% if bk_stts.ls_mb_text != 0 %}letter-spacing:{{bk_stts.ls_mb_text}}px;{% endif %}min-height:{{bk_stts.h_mb}}px;font-size: {{bk_stts.fs_mb_text}}px;color: {{bk_stts.cl_txt}};background-color: {{bk_stts.cl_bg}};border-color: {{bk_stts.cl_bd}}
           }
           .nt_se_{{sid}} .imtt4_btn.btn_icon_true:after {color: {{bk_stts.cl_txt}} }
           @media (min-width: 768px) {.nt_se_{{sid}} .imtt4_btn { padding-left:{{bk_stts.pd}}px;padding-right:{{bk_stts.pd}}px;border-radius:{{bk_stts.bdr}}px;{% if bk_stts.ls_text != 0 %}letter-spacing:{{bk_stts.ls_text}}px;{% endif %}font-weight: {{bk_stts.fw_text}};min-height:{{bk_stts.h}}px;font-size: {{bk_stts.fs_text}}px }}
        {%- endcase -%}
    {%- endfor -%}
  
  {%- endstyle -%}
  
